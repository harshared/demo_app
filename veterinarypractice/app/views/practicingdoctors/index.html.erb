<h1>Listing practicingdoctors</h1>

<table>
  <thead>
    <tr>
      <th>Nameofpet</th>
      <th>Typeofpet</th>
      <th>Breed</th>
      <th>Age</th>
      <th>Weight</th>
      <th>Dateoflastvisit</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @practicingdoctors.each do |practicingdoctor| %>
      <tr>
        <td><%= practicingdoctor.Nameofpet %></td>
        <td><%= practicingdoctor.typeofpet %></td>
        <td><%= practicingdoctor.breed %></td>
        <td><%= practicingdoctor.age %></td>
        <td><%= practicingdoctor.weight %></td>
        <td><%= practicingdoctor.dateoflastvisit %></td>
        <td><%= link_to 'Show', practicingdoctor %></td>
        <td><%= link_to 'Edit', edit_practicingdoctor_path(practicingdoctor) %></td>
        <td><%= link_to 'Destroy', practicingdoctor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Practicingdoctor', new_practicingdoctor_path %>
